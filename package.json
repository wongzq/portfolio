{
  "title": "Creative",
  "name": "wongzq-portfolio",
  "version": "6.0.3",
  "scripts": {
    "deploy": "gh-pages -d dist",
    "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
    "build:assets": "node scripts/build-assets.js",
    "build:pug": "node scripts/build-pug.js",
    "build:scripts": "node scripts/build-scripts.js",
    "build:scss": "node scripts/build-scss.js",
    "clean": "node scripts/clean.js",
    "start": "npm run build && node scripts/start.js",
    "start:debug": "npm run build && node scripts/start-debug.js"
  },
  "description": "Creative is a stylish one page HTML theme built with Bootstrap.",
  "keywords": [
    "css",
    "sass",
    "scss",
    "html",
    "pug",
    "responsive",
    "theme",
    "template"
  ],
  "homepage": "https://wongzq.github.io/portfolio",
  "license": "MIT",
  "author": "Start Bootstrap",
  "contributors": [
    "David Miller (http://davidmiller.io/)"
  ],
  "dependencies": {
    "bootstrap": "5.1.3",
    "gh-pages": "^3.2.3"
  },
  "devDependencies": {
    "autoprefixer": "10.4.0",
    "browser-sync": "^2.27.7",
    "chokidar": "3.5.2",
    "concurrently": "6.4.0",
    "postcss": "8.4.4",
    "prettier": "2.5.0",
    "pug": "3.0.2",
    "sass": "1.44.0",
    "shelljs": "0.8.4",
    "upath": "2.0.1"
  },
  "main": "index.js"
}
